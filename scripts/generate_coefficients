des=[0 1 0];
wate=[1 1 1];
[hn]=eqfir(nf,bedge,des,wate);
hn=hn;
//hn=round(hn);
[hm,fr]=frmag(hn,256);
plot(fr,hm)

scf(3);

if getos() == 'Windows' then unix('del coefficients_4000_6000');
else unix('rm -f coefficients_4000_6000'); end
write('coefficients_4000_6000',strcat(string(hn),','))

