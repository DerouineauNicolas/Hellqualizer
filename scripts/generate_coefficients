clear;
clc;
clf();
scf(0);

nf=63;
Fs=44100;
bedge=[0,2000/Fs;2100/Fs,15000/Fs;15100/Fs,22000/Fs];
des=[1 0 0];
wate=[1 1 1];
[hn]=eqfir(nf,bedge,des,wate);
hn=hn;
//hn=round(hn);
[hm,fr]=frmag(hn,256);
plot(fr,hm)

if getos() == 'Windows' then unix('del coefficients_0_2000');
else unix('rm -f coefficients_0_2000'); end
write('coefficients_0_2000',strcat(string(hn),','))

scf(1);

nf=63;
Fs=44100;
bedge=[0,2000/Fs;2100/Fs,4000/Fs;4100/Fs,22000/Fs];
des=[0 1 0];
wate=[1 1 1];
[hn]=eqfir(nf,bedge,des,wate);
hn=hn;
//hn=round(hn);
[hm,fr]=frmag(hn,256);
plot(fr,hm)

if getos() == 'Windows' then unix('del coefficients_2000_4000');
else unix('rm -f coefficients_2000_4000'); end
write('coefficients_2000_4000',strcat(string(hn),','))

scf(2);

